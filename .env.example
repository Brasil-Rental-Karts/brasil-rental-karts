# Ambiente da aplicação
# ====================
NODE_ENV=development

# Configurações do Supabase
# ====================
NEXT_PUBLIC_SUPABASE_URL=sua_url_do_supabase_aqui
NEXT_PUBLIC_SUPABASE_ANON_KEY=sua_chave_anonima_do_supabase_aqui
SUPABASE_SERVICE_KEY=sua_chave_de_servico_do_supabase_aqui

# Configurações do Sentry
# ====================
# DSN pública para integração cliente/servidor
NEXT_PUBLIC_SENTRY_DSN=seu_dsn_do_sentry_aqui
NEXT_PUBLIC_SENTRY_URL=https://seu-org.sentry.io

# Configurações da organização Sentry
SENTRY_ORG=seu-org                    # Nome da organização (Ex: acme em acme.sentry.io)
SENTRY_PROJECT=seu-projeto            # Nome do projeto no Sentry
SENTRY_AUTH_TOKEN=seu_token_aqui      # Token de autenticação (https://sentry.io/settings/account/api/auth-tokens/)
SENTRY_SUPPRESS_TURBOPACK_WARNING=1   # Desativa avisos do Turbopack

# Configurações do SonarQube
# ====================
SONAR_TOKEN=your_sonar_token_here     # Token de autenticação
SONAR_HOST_URL=http://localhost:9000  # URL do servidor 